citHeader("To cite tensorEVD in publications use:")

bibentry(
    bibtype = "Article",
    title = "A fast algorithm to factorize high-dimensional Tensor Product matrices used in Genetic Models",
    author = c(
        person("Marco", "Lopez-Cruz"),
        person("Paulino", "Perez-Rodriguez"),
        person("Gustavo", "de los Campos")
    ),
    journal = "G3 Genes|Genomes|Genetics",
    year = "2024",
    volume = "14",
    number = "3",
    pages = "1-8",
    textVersion = paste(
        "Lopez-Cruz M, Perez-Rodriguez P & de los Campos G, 2024. ",
        "A fast algorithm to factorize high-dimensional Tensor Product matrices used in Genetic Models. ",
        "G3 Genes|Genomes|Genetics 14(3): 1-8."
    )
)
